#!/usr/pkg/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'postgresfixture==0.4.2','console_scripts','postgresfixture'
__requires__ = 'postgresfixture==0.4.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('postgresfixture==0.4.2', 'console_scripts', 'postgresfixture')()
    )
