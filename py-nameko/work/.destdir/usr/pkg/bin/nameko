#!/usr/pkg/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'nameko==2.13.0','console_scripts','nameko'
__requires__ = 'nameko==2.13.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('nameko==2.13.0', 'console_scripts', 'nameko')()
    )
