#!/usr/pkg/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'humanfriendly==8.2','console_scripts','humanfriendly'
__requires__ = 'humanfriendly==8.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('humanfriendly==8.2', 'console_scripts', 'humanfriendly')()
    )
